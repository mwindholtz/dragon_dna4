<% content_for :header, "Editing Monster" %>
<% content_for :second_nav do -%>
  <li><%= link_to 'Show', @monster %> </li>
  <li><%= link_to 'Back', monsters_path %></li>
<%- end -%>


<%= form_for(@monster) do |f| %>
  <%= display_errors(f) %>

  <%= render :partial => 'monsters/form', :locals => {:f => f} %>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

